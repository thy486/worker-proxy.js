{
    "singleQuote": true,
    "printWidth": 120,
    "semi": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "endOfLine": "lf",
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "tabWidth": 2
            }
        },
        {
            "files": "*.ts",
            "options": {
                "tabWidth": 4
            }
        }
    ]
}
